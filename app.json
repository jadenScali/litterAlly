{
  "expo": {
    "name": "LitterAlly",
    "slug": "litterAllyDone",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jadenexpo.litterAllyDone",
      "buildNumber": "3"
    },
    "android": {
      "package": "com.jadenexpo.litterAllyDone",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "adda7ee8-8465-4246-945d-a5ced225a6bc"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-build-properties",
      [
        "expo-camera",
        {
          "cameraPermission": "This app uses the camera to scan objects and determine how to dispose of them."
        }
      ],
      "expo-font",
      "expo-asset"
    ]
  }
}
